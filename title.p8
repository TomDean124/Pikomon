pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

wait_time = 3
timer = 0

function _init()
cls() 
end

function _update()
timer += 1/30 
if timer >= wait_time then
load("picomon.p8")
run()
end
end

function _draw()
cls(0) 
color(7)
print("to mr.krugar", 64 - (#"to mr.krugar"*2), 64)
end

